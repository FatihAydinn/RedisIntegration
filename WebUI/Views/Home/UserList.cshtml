@model IEnumerable<UserViewModel>

<table class="table">
    <thead>
        <tr>
            <th scope="row">Name</th>
            <th scope="row">Email</th>
            <th scope="row">Address</th>
            <th scope="row">Age</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Name</th>
                    <td>@item.Email</td>
                    <td>@item.Address</td>
                    <td>@item.Age</td>
                </tr>
            }
        }
    </tbody>
</table>